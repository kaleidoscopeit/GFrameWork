<?xml version="1.0"  encoding="UTF-8" ?>

<root:htmlview
	title="G-Framework Web3.0 Toolkit"
	style="overflow:auto;">

	<pack:vlayout>
		<pack:vlaycell height="30">
		</pack:vlaycell>

		<pack:vlaycell id="tast">

			<pack:stack default="0">

				<pack:stackelm >
 				  <base:codesnippet
				    id="xml_code"
				    hilight="xml"
					class="mono12"
				    style="border:1px dashed #B0BEC7;background-color:#F1F5F6;"/>
				</pack:stackelm>


				<pack:stackelm >
 				  <base:codesnippet
				    id="php_code"
				    hilight="php"
				    style="border:1px dashed #B0BEC7;font:10px mono;background-color:#F1F5F6;"/>
				</pack:stackelm>
			</pack:stack>



		</pack:vlaycell>

	</pack:vlayout>
</root:htmlview>
