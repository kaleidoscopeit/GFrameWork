<?xml version="1.0"  encoding="UTF-8" ?>

<base:htmlview
	title="G-Framework Web3.0 Toolkit"
	style="overflow:auto;"
	id="view_source">

	<pack:vlayout>
		<pack:vlaycell height="30">
		</pack:vlaycell>
		
		<pack:vlaycell>

			<pack:stack default="1">
				

				<pack:stackelm>
				  <base:codesnippet
				    id="xml_code"
				    hilight="xml" 
				    style="border:1px dashed #B0BEC7;font:10px mono;background-color:#F1F5F6;"/>		
				</pack:stackelm>				
			</pack:stack>

				<pack:stackelm>							
				  <base:codesnippet
				    id="php_code"
				    hilight="xml" 
				    style="border:1px dashed #B0BEC7;font:10px mono;background-color:#F1F5F6;"/>		
				</pack:stackelm>

			

	
		</pack:vlaycell>

	</pack:vlayout>
</base:htmlview>